# -----------------------------------------------------------------------------
#                            FILES TO READ
# -----------------------------------------------------------------------------


set universes 4
u1 file_name_u1
u2 file_name_u2
u3 file_name_u3
u4 file_name_u4

# -----------------------------------------------------------------------------
#                            READING RULES
# -----------------------------------------------------------------------------


set signs state = /+ +/, attribute = /< >/ , varIn = /" "/ , varOut = /^ ^/

set format state 4.3f attr = 5.5e var = 3d

set option 1


set decorators time 3 0
* ----------------------------------------------------------------
* BURNUP  /" "/ 
* ----------------------------------------------------------------


set decorators energy 
*   Group /" "/



